//PM10 등급 리턴
   //pm10 : 1(0~30), 2(30~80), 3(80~150), 4(150~500)
   //pm10 : 1(0~25), 2(25~50), 3(50~76), 4(75~100)
   private int getGradePm10(Double pm10){
//    if(pm10 >= 150)
//       return 4;  //매우나쁨
//    else if(pm10 >= 80)
//       return 3;  //보통
//    else if(pm10 >= 30)
//       return 2;  //보통
//    else
//       return 1;  //좋음
      if(pm10 >= 75)
         return 4;  //매우나쁨
      else if(pm10 >= 50)
         return 3;  //보통
      else if(pm10 >= 25)
         return 2;  //보통
      else
         return 1;  //좋음
   }


    //CO2 등급 리턴
   //co2 : 1(0~450), 2(450~1000), 3(1000~3000), 4(3000~5000)
   //co2 : 1(0~25), 2(25~50), 3(50~76), 4(75~100)
   private int getGradeCo2(Double co2){
//    if(co2 >= 3000)
//       return 4;  //매우나쁨
//    else if(co2 >= 1000)
//       return 3;  //보통
//    else if(co2 >= 450)
//       return 2;  //보통
//    else
//       return 1;  //좋음
      
      if(co2 >= 75)
         return 4;  //매우나쁨
      else if(co2 >= 50)
         return 3;  //보통
      else if(co2 >= 25)
         return 2;  //보통
      else
         return 1;  //좋음

   }


    //VOCs 등급 리턴
   //vocs : 1(0~55), 2(55~78), 3(78~87), 4(87~100)
   //vocs : 1(0~25), 2(25~50), 3(50~76), 4(75~100)
   private int getGradeVocs(Double vocs){
//    if(vocs >= 87)
//       return 4;  //매우나쁨
//    else if(vocs >= 78)
//       return 3;  //보통
//    else if(vocs >= 55)
//       return 2;  //보통
//    else
//       return 1;  //좋음
      
      if(vocs >= 75)
         return 4;  //매우나쁨
      else if(vocs >= 50)
         return 3;  //보통
      else if(vocs >= 25)
         return 2;  //보통
      else
         return 1;  //좋음
   }

